  "keywords": [
    "faceit",
    "cs2",
    "statistics",
    "analysis",
    "gaming"
  ],
  "homepage": "https://pattmsc.online",
  "bugs": {
    "url": "https://github.com/pat1one/faceit-ai-bot/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pat1one/faceit-ai-bot.git"
  },
  "license": "Source-available (see LICENSE)",
  "author": "pattmsc",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@mswjs/interceptors": "^0.39.7",
    "@swc/core": "^1.15.1",
    "@swc/helpers": "^0.5.17",
    "chart.js": "^4.5.1",
    "i18next": "^25.6.1",
    "next": "^15.5.4",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.2.4",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.28.5",
    "@jest/globals": "^30.2.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chart.js": "^2.9.41",
    "@types/jest": "^29.5.14",
    "@types/react": "18.2.21",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.11.0",
    "esbuild": "^0.24.0",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^30.1.2",
    "babel-loader": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-fetch-mock": "^3.0.3",
    "js-yaml": "^4.1.1",
    "msw": "^2.11.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.2.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4",
    "whatwg-fetch": "^3.6.20",
    "yazl": "^2.5.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "webpack:build": "npx webpack --config webpack.config.js",
    "test": "jest --passWithNoTests",
    "test:unit": "pytest tests/unit -v",
    "test:integration": "pytest tests/integration -v",
    "test:all": "pytest tests -v --cov=src/server --cov-report=html --cov-report=term-missing",
    "test:coverage": "pytest tests -m \"not e2e\" --cov=src/server --cov-report=html --cov-report=term-missing --cov-fail-under=70",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "build:all": "npm run build && npm run webpack:build",
    "build:extension": "node build-extension.js",
    "build:extension:zip": "npm run build:extension && node build-extension-zip.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "deploy": "bash deploy.sh",
    "dev:db": "bash dev.sh",
    "build:docs": "mkdir -p docs && cp README.md docs/index.md && echo '# Documentation\n\nThis is the documentation for Faceit AI Bot.\n\n## Main Site\nVisit our main site at: https://pattmsc.online\n\n## Features\n- Player statistics analysis\n- Demo upload and analysis\n- Teammate search\n- Real-time data processing\n\n## Getting Started\nCheck the main README.md for installation and usage instructions.' > docs/overview.md"
  }
}
